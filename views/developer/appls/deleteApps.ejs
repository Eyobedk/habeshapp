<!doctype html>
<html class="fixed">

<head>

    <!-- Basic -->
    <meta charset="UTF-8">

    <title>Developer Dashboard</title>
    <meta name="keywords" content="HTML5 Admin Template" />
    <meta name="description" content="JSOFT Admin - Responsive HTML5 Template">
    <meta name="author" content="JSOFT.net">

    <link href="https://fonts.googleapis.com/css2?family=Poppins&display=swap" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />

    <!-- Web Fonts  -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800|Shadows+Into+Light"
        rel="stylesheet" type="text/css">

    <!-- Vendor CSS -->
    <link rel="stylesheet" href="/bootstrap/bootstrap.css" />
    <!-- My css -->
    <link rel="stylesheet" href="/css/pannel.css">
    <!-- Theme CSS -->
    <link rel="stylesheet" href="/bootstrap/theme.css" />



    <!-- Skin CSS -->
    <link rel="stylesheet" href="/bootstrap/default.css" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.1/css/font-awesome.min.css">
    <link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/js/bootstrap.min.js"></script>
    <script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
</head>

<body>
    <section class="body">

        <!-- start: header -->
        <header class="header">
            <div class="logo-container">
                <a class="logo" style="text-decoration: none;">
                    <img src="/images/Logo.png" height="35" alt="JSOFT Admin" />
                    <strong class="hider" style="margin-left:20px;padding-top:100px !important;font-size: medium;">HABESHAPP</strong>

                </a>
                <div class="header-right">

                    <div id="userbox" class="userbox">
                        <a href="#" data-toggle="dropdown">
                            <%if(dev) {%>
                            <% console.log("The ejs"+JSON.stringify(dev))%>
                            <div class="profile-info" data-lock-name="John Doe" data-lock-email="johndoe@JSOFT.com">
                                <span class="name"><%= dev.email %></span>
                                <span class="role">Developer</span>
                            </div>
                            <% } else { %><div style="display: none;"></div><% } %>

                            <i class="fa custom-caret"></i>
                        </a>

                        <div class="dropdown-menu">
                            <ul class="list-unstyled">
                                <li class="divider"></li>
                                <li>
                                    <a role="menuitem" tabindex="-1" href="#"><i class="fa fa-power-off"></i> Logout</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>

            <!-- start: search & user box -->

            <!-- end: search & user box -->
        </header>
        <!-- end: header -->

       
        <div class="inner-wrapper">

            <section role="main" class="content-body">
                <header class="page-header">
                    <h2><a href="/pannel" style="text-decoration: none;color: white;">Dashboard</a></h2>

                </header>
                
                <% if(typeof deleteMessage != 'undefined'){ %>
                <div class="alert alert-success" role="alert">
                   <%= deleteMessage %>
                  </div>
                  <% } %>

                  <div class="popup">
                    <!-- <button id="close">&times;</button> -->
                    <h2 style="color: red;">1 WEEK NOT REACHED</h2>
                    <p>
                        You can not delete a published app that has not been on the platform for atleast one week.
                    </p>
                  <center> <div id="clothok" class="okButton">Ok</div></center> 
                </div>

				
                    <center>
                        <div class="container-fluid">
                            <div class="row">
                        <%
                        if(typeof ToSend != 'undefined') { console.log(ToSend)%>
                        <% ToSend.forEach((file)=>{ %>
                        <div class="col-md-3 col-sm-4">
                            <div class="wrimagecard wrimagecard-topimage" style="background:rgb(255, 74, 74) !important;border-radius:27px;">
                                <a href="delete/<%= file[2] %>"  style="text-decoration: none;">
                                <div class="wrimagecard-topimage_header" style="background-color: rgb(255, 155, 155)">
                                    <center><img style="opacity: 50; height: 100%;
                                        width: 100%;
                                        object-fit: contain;" src=<%=file[1]%> ></img></center>
                                </div>
                                <div class="wrimagecard-topimage_title">
                                 <center> <h4><b style="color: white"> <%= file[0].replace(/[_]+/g, ' ') %> </b> </center>
                                  <div class="pull-right badge" id="WrControls"></div></h4>
                                </div>
                              </a>
                            </div>
                      </div>
                      <% }) %>
                      <% } %>
        
                        </center>
                    <% if(typeof oneWeeklimit != 'undefined') {%>
                        <script type="text/javascript">
                            document.querySelector(".popup").style.display = "block";
                        
                        
                        document.querySelector("#clothok").addEventListener("click", function(){
                            document.querySelector(".popup").style.display = "none";
                            window.history.go(-1); return false;
                        });
                        </script>
                        <% } %>

            </section>
        </div>

    </section>


    <!-- Vendor -->
    <script src="/js/jquery.js"></script>
    <script src="/js/jquery.browser.mobile.js"></script>
    <script src="/js/bootstrap.js"></script>
    <script src="/js/theme.js"></script>

    <script src="/js/theme.init.js"></script>
</body>

</html>